{
  "name": "electron-antd",
  "appId": "org.electron.electron-antd",
  "version": "1.0.0",
  "buildVersion": "0001",
  "private": false,
  "main": "dist/main/main.js",
  "author": "lanten",
  "scripts": {
    "dev": "cross-env NODE_ENV=development BUILD_ENV=dev npm run ts:dev",
    "build:dev": "cross-env NODE_ENV=production BUILD_ENV=dev npm run ts:build",
    "build": "cross-env NODE_ENV=production BUILD_ENV=prod npm run ts:build",
    "ts:dev": "npm run ts:compile && node build/.out/tasks/dev-server.js",
    "ts:build": "npm run ts:compile && node build/.out/tasks/build.js",
    "ts:compile": "tsc --project build/build.tsconfig.json",
    "lint": "eslint --ext .ts,.tsx src"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@electron/remote": "^2.0.5",
    "@rollup/plugin-inject": "^4.0.4",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-react": "^1.2.0",
    "antd": "^4.19.1",
    "axios": "^0.26.0",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "electron": "^17.1.1",
    "electron-builder": "^22.14.13",
    "electron-log": "^4.4.6",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "less": "^4.1.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "remixicon": "^2.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanten/electron-antd.git"
  },
  "bugs": {
    "url": "https://github.com/lanten/electron-antd/issues"
  },
  "homepage": "https://github.com/lanten/electron-antd#readme"
}
